{
  "name": "figma-select-layers",
  "version": "0.0.0",
  "description": "A Figma plugin to select layers based on similarity, name, or type",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "select",
    "select-layers"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-select-layers"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "create-figma-plugin-build",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.0.5"
  },
  "create-figma-plugin": {
    "id": "figma-select-layers",
    "name": "Select Layers",
    "menu": [
      {
        "name": "Select Similar Layers",
        "command": "select-similar-layers/command.js"
      },
      {
        "name": "Select Layers by Name",
        "command": "select-layers-by-name/command.js",
        "ui": "select-layers-by-name/ui.js"
      },
      {
        "name": "Select Layers by Type",
        "menu": [
          {
            "name": "Select Components",
            "command": "select-layers-by-type/command.js",
            "handler": "component"
          },
          {
            "name": "Select Instances",
            "command": "select-layers-by-type/command.js",
            "handler": "instance"
          },
          "-",
          {
            "name": "Select Frames",
            "command": "select-layers-by-type/command.js",
            "handler": "frame"
          },
          {
            "name": "Select Groups",
            "command": "select-layers-by-type/command.js",
            "handler": "group"
          },
          {
            "name": "Select Slices",
            "command": "select-layers-by-type/command.js",
            "handler": "slice"
          },
          "-",
          {
            "name": "Select Rectangles",
            "command": "select-layers-by-type/command.js",
            "handler": "rectangle"
          },
          {
            "name": "Select Lines",
            "command": "select-layers-by-type/command.js",
            "handler": "line"
          },
          {
            "name": "Select Ellipses",
            "command": "select-layers-by-type/command.js",
            "handler": "ellipse"
          },
          {
            "name": "Select Polygons",
            "command": "select-layers-by-type/command.js",
            "handler": "polygon"
          },
          {
            "name": "Select Stars",
            "command": "select-layers-by-type/command.js",
            "handler": "star"
          },
          {
            "name": "Select Vectors",
            "command": "select-layers-by-type/command.js",
            "handler": "vector"
          },
          {
            "name": "Select Boolean Operation Layers",
            "command": "select-layers-by-type/command.js",
            "handler": "booleanOperation"
          },
          "-",
          {
            "name": "Select Images",
            "command": "select-layers-by-type/command.js",
            "handler": "image"
          },
          "-",
          {
            "name": "Select Text Layers",
            "command": "select-layers-by-type/command.js",
            "handler": "text"
          }
        ]
      },
      "-",
      {
        "name": "Select Hidden Layers",
        "command": "select-layers-by-type/command.js",
        "handler": "hidden"
      },
      {
        "name": "Select Locked Layers",
        "command": "select-layers-by-type/command.js",
        "handler": "locked"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
