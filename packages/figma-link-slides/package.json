{
  "name": "figma-link-slides",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-link-slides"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run clean && build-figma-plugin --typecheck --minify",
    "clean": "rimraf '*.log' build 'src/**/*.css.d.ts'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --loglevel error --write '*.json'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "watch": "npm run clean && build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^2.1.1"
  },
  "figma-plugin": {
    "name": "Link Slides",
    "id": "figma-link-slides",
    "editorType": [
      "figma"
    ],
    "menu": [
      {
        "name": "Link Slides",
        "main": "src/link-slides/main.ts"
      },
      {
        "name": "Unlink Slides",
        "main": "src/unlink-slides/main.ts"
      },
      "-",
      {
        "name": "Settings",
        "main": "src/settings/main.ts",
        "ui": "src/settings/ui.ts"
      }
    ],
    "relaunchButtons": {
      "link-slides": {
        "name": "Link Slides",
        "main": "src/link-slides/main.ts"
      },
      "unlink-slides": {
        "name": "Unlink Slides",
        "main": "src/unlink-slides/main.ts"
      }
    }
  }
}
