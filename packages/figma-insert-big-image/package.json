{
  "name": "figma-insert-big-image",
  "version": "1.3.0",
  "description": "A Figma plugin to insert and retain the original resolution of big images",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-insert-big-image"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin --minify --typecheck",
    "clean": "rm -rf '*.log' build manifest.json 'src/**/*.css.d.ts'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.css' && prettier --loglevel error --write 'src/**/*.css' package.json tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}' && stylelint 'src/**/*.css'",
    "watch": "yarn run clean && build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "latest",
    "@create-figma-plugin/utilities": "latest",
    "preact": "^10.5.13"
  },
  "figma-plugin": {
    "name": "Insert Big Image",
    "id": "799646392992487942",
    "main": "src/main.ts",
    "ui": "src/ui.ts"
  }
}
