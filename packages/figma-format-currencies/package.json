{
  "name": "figma-format-currencies",
  "version": "0.0.0",
  "description": "A Figma plugin to format and convert between currencies",
  "keywords": [
    "currency",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "i18n",
    "internationalisation",
    "l20n",
    "localization",
    "money"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-format-currencies"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "create-figma-plugin-build",
    "prebuild": "yarn run scripts",
    "pretest": "yarn run scripts",
    "prewatch": "yarn run scripts",
    "scripts": "node scripts/build-symbol-to-iso-codes-map.js > src/data/symbol-to-iso-codes-map.json && node scripts/build-iso-code-to-decimal-places-map.js > src/data/iso-code-to-decimal-places-map.json",
    "test": "ava",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "*",
    "figma-ui": "*",
    "preact": "^10.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "verbose": true,
    "require": [
      "esm"
    ]
  },
  "create-figma-plugin": {
    "id": "figma-format-currencies",
    "name": "Format Currencies",
    "menu": [
      {
        "name": "Format Short",
        "command": "format-short/command.js",
        "ui": "format-short/ui.js"
      },
      {
        "name": "Format Explicit",
        "command": "format-explicit/command.js",
        "ui": "format-explicit/ui.js"
      },
      {
        "name": "Fix Currency Format",
        "command": "fix-currency-format/command.js",
        "ui": "fix-currency-format/ui.js"
      },
      "-",
      {
        "name": "Settings",
        "command": "settings/command.js",
        "ui": "settings/ui.js"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
