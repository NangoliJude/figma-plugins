{
  "name": "figma-set-dimension",
  "version": "0.0.0",
  "description": "A Figma plugin to set the width or height of layers with a single click",
  "keywords": [
    "create-figma-plugin",
    "dimension",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "set-dimension"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-set-dimension"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "create-figma-plugin-build",
    "fix": "concurrently --raw 'yarn run fix-css' 'yarn run fix-js'",
    "fix-css": "prettier --write 'src/**/*.scss'",
    "fix-js": "prettier-standard '{scripts,src,test}/**/*.js'",
    "lint": "concurrently --raw 'yarn run lint-css' 'yarn run lint-js'",
    "lint-css": "stylelint 'src/**/*.scss'",
    "lint-js": "standardx '{scripts,src,test}/**/*.js'",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.0.5"
  },
  "create-figma-plugin": {
    "id": "figma-set-dimension",
    "name": "Set Dimension",
    "menu": [
      {
        "name": "Set Dimension",
        "command": "set-dimension/command.js",
        "ui": "set-dimension/ui.js"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
