{
  "name": "figma-distribute-layers",
  "version": "1.5.0",
  "description": "A Figma plugin to distribute layers in a given direction and offset",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-distribute-layers"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin",
    "clean": "rimraf '*.log' build manifest.json",
    "fix": "standardx --fix 'src/**/*.{ts,tsx}' && prettier-standard 'src/**/*.{ts,tsx}' && prettier --write package.json tsconfig.json",
    "lint": "standardx 'src/**/*.{ts,tsx}'",
    "watch": "build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.4.1"
  },
  "figma-plugin": {
    "id": "767379264700569551",
    "name": "Distribute Layers",
    "menu": [
      {
        "name": "Distribute Layers Left",
        "main": "distribute-layers-left/main",
        "ui": "distribute-layers-left/ui"
      },
      {
        "name": "Distribute Layers Right",
        "main": "distribute-layers-right/main",
        "ui": "distribute-layers-right/ui"
      },
      {
        "name": "Distribute Layers Up",
        "main": "distribute-layers-up/main",
        "ui": "distribute-layers-up/ui"
      },
      {
        "name": "Distribute Layers Down",
        "main": "distribute-layers-down/main",
        "ui": "distribute-layers-down/ui"
      }
    ]
  },
  "standardx": {
    "ignore": [
      "src/**/*.scss.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
