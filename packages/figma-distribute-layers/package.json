{
  "name": "figma-distribute-layers",
  "version": "1.6.0",
  "description": "A Figma plugin to distribute layers in a given direction and offset",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-distribute-layers"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin --minify --typecheck",
    "clean": "rm -rf '*.log' build manifest.json 'src/**/*.css.d.ts'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --loglevel error --write package.json tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "watch": "yarn run clean && build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "latest",
    "@create-figma-plugin/utilities": "latest",
    "preact": "^10.5.13"
  },
  "figma-plugin": {
    "name": "Distribute Layers",
    "id": "767379264700569551",
    "menu": [
      {
        "name": "Distribute Layers Left",
        "main": "src/distribute-nodes-left/main.ts",
        "ui": "src/distribute-nodes-left/ui.ts"
      },
      {
        "name": "Distribute Layers Right",
        "main": "src/distribute-nodes-right/main.ts",
        "ui": "src/distribute-nodes-right/ui.ts"
      },
      {
        "name": "Distribute Layers Up",
        "main": "src/distribute-nodes-up/main.ts",
        "ui": "src/distribute-nodes-up/ui.ts"
      },
      {
        "name": "Distribute Layers Down",
        "main": "src/distribute-nodes-down/main.ts",
        "ui": "src/distribute-nodes-down/ui.ts"
      }
    ]
  }
}
