{
  "name": "figma-format-currency",
  "version": "1.0.2",
  "description": "A Figma plugin to format and convert between currencies",
  "keywords": [
    "currency",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "i18n",
    "internationalisation",
    "l20n",
    "localization",
    "money"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-format-currency"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "create-figma-plugin-build",
    "build-iso-codes": "node scripts/build-iso-codes.js",
    "fix": "concurrently --raw 'yarn run fix-css' 'yarn run fix-js'",
    "fix-css": "prettier --write 'src/**/*.scss'",
    "fix-js": "prettier-standard '{scripts,src,test}/**/*.js'",
    "lint": "concurrently --raw 'yarn run lint-css' 'yarn run lint-js'",
    "lint-css": "stylelint 'src/**/*.scss'",
    "lint-js": "standardx '{scripts,src,test}/**/*.js'",
    "prebuild": "yarn run build-iso-codes",
    "pretest": "yarn run build-iso-codes",
    "prewatch": "yarn run build-iso-codes",
    "test": "ava",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "@sindresorhus/class-names": "^1.1.0",
    "preact": "^10.1.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "verbose": true,
    "require": [
      "esm"
    ]
  },
  "create-figma-plugin": {
    "id": "780785006732555859",
    "name": "Format Currency",
    "menu": [
      {
        "name": "Format Currency",
        "command": "format-currency/command.js",
        "ui": "format-currency/ui.js"
      },
      {
        "name": "Convert Currency",
        "command": "convert-currency/command.js",
        "ui": "convert-currency/ui.js"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
