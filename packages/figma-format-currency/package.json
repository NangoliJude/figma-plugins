{
  "name": "figma-format-currency",
  "version": "1.3.0",
  "description": "A Figma plugin to format and convert between currencies",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-format-currency"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean-build && yarn run generate-currencies-ts && build-figma-plugin --minify --typecheck",
    "generate-currencies-ts": "ts-node scripts/generate-currencies-ts.ts > src/utilities/data/currencies.ts && eslint --fix src/utilities/data/currencies.ts",
    "clean": "yarn run clean-build && yarn run clean-test",
    "clean-build": "rm -rf '*.log' build",
    "clean-test": "rm -rf '*.log' .nyc_output coverage",
    "fix": "eslint --fix '{scripts,src,test}/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.css' && prettier --loglevel error --write 'src/**/*.css' '*.json'",
    "lint": "eslint '{scripts,src,test}/**/*.{ts,tsx}' && stylelint 'src/**/*.css'",
    "test": "yarn run clean-test && yarn run generate-currencies-ts && TS_NODE_PROJECT='test/tsconfig.json' tap 'test/**/*.ts' --coverage-report html --coverage-report text --no-browser --no-check-coverage --reporter terse --timeout 120 --ts",
    "watch": "yarn run clean-build && yarn run generate-currencies-ts && build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "latest",
    "@create-figma-plugin/utilities": "latest",
    "@sindresorhus/class-names": "^1.1.0",
    "preact": "^10.5.13"
  },
  "figma-plugin": {
    "name": "Format Currency",
    "id": "780785006732555859",
    "menu": [
      {
        "name": "Format Currency",
        "main": "src/format-currency/main.ts",
        "ui": "src/format-currency/ui.ts"
      },
      {
        "name": "Convert Currency",
        "main": "src/convert-currency/main.ts",
        "ui": "src/convert-currency/ui.ts"
      },
      "-",
      {
        "name": "Reset Plugin",
        "main": "src/reset-plugin/main.ts"
      }
    ]
  }
}
