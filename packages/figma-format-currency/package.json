{
  "name": "figma-format-currency",
  "version": "1.3.0",
  "description": "A Figma plugin to format and convert between currencies",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-format-currency"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run clean-build && npm run generate-currencies-ts && build-figma-plugin --typecheck --minify",
    "generate-currencies-ts": "node --loader ts-node/esm scripts/generate-currencies-ts.ts > src/utilities/data/currencies.ts && eslint --fix src/utilities/data/currencies.ts",
    "clean": "npm run clean-build && npm run clean-test",
    "clean-build": "rimraf '*.log' build",
    "clean-test": "rimraf '*.log'",
    "fix": "eslint --fix '{scripts,src,test}/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.css' && prettier --loglevel error --write 'src/**/*.css' '*.json'",
    "lint": "eslint '{scripts,src,test}/**/*.{ts,tsx}' && stylelint 'src/**/*.css'",
    "test": "npm run clean-test && npm run generate-currencies-ts && ava 'test/**/*.ts'",
    "watch": "npm run clean-build && npm run generate-currencies-ts && build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^2.1.1",
    "@create-figma-plugin/utilities": "^2.1.1",
    "preact": "^10.10.1"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "failFast": true,
    "nodeArguments": [
      "--loader",
      "ts-node/esm"
    ],
    "timeout": "2m"
  },
  "figma-plugin": {
    "name": "Format Currency",
    "id": "780785006732555859",
    "editorType": [
      "figma"
    ],
    "menu": [
      {
        "name": "Format Currency",
        "main": "src/format-currency/main.ts",
        "ui": "src/format-currency/ui.ts"
      },
      {
        "name": "Convert Currency",
        "main": "src/convert-currency/main.ts",
        "ui": "src/convert-currency/ui.ts"
      },
      "-",
      {
        "name": "Reset Plugin",
        "main": "src/reset-plugin/main.ts"
      }
    ]
  }
}
