{
  "name": "figma-draw-mask-under-selection",
  "version": "1.1.0",
  "description": "A Figma plugin to draw a mask under the selection",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-draw-mask-under-selection"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run clean && build-figma-plugin --typecheck --minify",
    "clean": "rimraf '*.log' build 'src/**/*.css.d.ts'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --loglevel error --write '*.json'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "watch": "npm run clean && build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^2.1.5"
  },
  "figma-plugin": {
    "name": "Draw Mask Under Selection",
    "id": "806532458729477508",
    "editorType": [
      "figma"
    ],
    "main": "src/main.ts"
  }
}
