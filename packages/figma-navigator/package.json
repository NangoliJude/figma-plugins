{
  "name": "figma-navigator",
  "version": "0.0.0",
  "description": "A Figma plugin to navigate between frames and components on the page",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "frame",
    "frame-navigator",
    "navigate",
    "navigator",
    "next",
    "previous"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-navigator"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "create-figma-plugin-build",
    "fix": "concurrently --raw 'yarn run fix-css' 'yarn run fix-js'",
    "fix-css": "prettier --write 'src/**/*.scss'",
    "fix-js": "prettier-standard '{scripts,src,test}/**/*.js'",
    "lint": "concurrently --raw 'yarn run lint-css' 'yarn run lint-js'",
    "lint-css": "stylelint 'src/**/*.scss'",
    "lint-js": "standardx '{scripts,src,test}/**/*.js'",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.1.1"
  },
  "create-figma-plugin": {
    "id": "figma-navigator",
    "name": "Navigator",
    "menu": [
      {
        "name": "Jump to Component/Frame",
        "command": "jump-to-layer/command.js",
        "ui": "jump-to-layer/ui.js"
      },
      "-",
      {
        "name": "Go to Previous Component/Frame",
        "command": "navigate-between-layers/command.js",
        "handler": "previous"
      },
      {
        "name": "Go to Next Component/Frame",
        "command": "navigate-between-layers/command.js",
        "handler": "next"
      },
      {
        "name": "Settings",
        "command": "settings/command.js",
        "ui": "settings/ui.js"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
