{
  "name": "figma-set-layer-size",
  "version": "2.0.0",
  "description": "A Figma plugin to precisely set the size of layers, while ignoring constraints",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-set-layer-size"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin",
    "clean": "rimraf '*.log' build manifest.json",
    "fix": "prettier-standard 'src/**/*.{ts,tsx}' && prettier --write package.json tsconfig.json",
    "lint": "standardx 'src/**/*.{ts,tsx}'",
    "watch": "build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.4.1"
  },
  "figma-plugin": {
    "id": "817982008781589678",
    "name": "Set Layer Size",
    "main": "main",
    "ui": "ui"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
