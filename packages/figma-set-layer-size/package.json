{
  "name": "figma-set-layer-size",
  "version": "2.1.0",
  "description": "A Figma plugin to precisely set the size of layers, while ignoring constraints",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-set-layer-size"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin --minify --typecheck",
    "clean": "rm -rf '*.log' build manifest.json 'src/**/*.css.d.ts'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --loglevel error --write package.json tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "watch": "yarn run clean && build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "latest",
    "@create-figma-plugin/utilities": "latest",
    "preact": "^10.5.13"
  },
  "figma-plugin": {
    "name": "Set Layer Size",
    "id": "817982008781589678",
    "main": "src/main.ts",
    "ui": "src/ui.ts"
  }
}
