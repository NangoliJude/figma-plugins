{
  "name": "figma-money",
  "version": "0.0.0",
  "description": "A Figma plugin to convert and format currencies",
  "keywords": [
    "currency",
    "figma",
    "figma-plugin",
    "figma-plugins",
    "i18n",
    "internationalisation",
    "l20n",
    "localization",
    "money"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-money"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "node scripts/build-symbol-to-iso-codes-map.js > src/data/symbol-to-iso-codes-map.json && create-figma-plugin-build",
    "watch": "create-figma-plugin-build --watch"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "*",
    "figma-ui": "*",
    "preact": "^10.0.0"
  },
  "create-figma-plugin": {
    "id": "figma-money",
    "name": "Money",
    "menu": [
      {
        "name": "Format Short",
        "command": "format-short/command.js",
        "ui": "format-short/ui.js"
      },
      {
        "name": "Format Explicit",
        "command": "format-explicit/command.js",
        "ui": "format-explicit/ui.js"
      },
      "-",
      {
        "name": "Reset",
        "command": "reset/command.js"
      }
    ]
  }
}
