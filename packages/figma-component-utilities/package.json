{
  "name": "figma-component-utilities",
  "version": "1.1.2",
  "description": "A Figma plugin to create and copy components, and replace layers with components",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
