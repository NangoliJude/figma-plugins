{
  "name": "figma-draw-slice-over-selection",
  "version": "1.5.0",
  "description": "A Figma plugin to draw a slice over the selection",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-draw-slice-over-selection"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin",
    "clean": "rimraf '*.log' build manifest.json",
    "fix": "standardx --fix 'src/**/*.{ts,tsx}' && prettier-standard 'src/**/*.{ts,tsx}' && prettier --write package.json tsconfig.json",
    "lint": "standardx 'src/**/*.{ts,tsx}'",
    "watch": "build-figma-plugin --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "*",
    "@create-figma-plugin/utilities": "*",
    "preact": "^10.4.1"
  },
  "figma-plugin": {
    "id": "767379335945775056",
    "name": "Draw Slice Over Selection",
    "main": "main",
    "ui": "ui"
  },
  "standardx": {
    "ignore": [
      "src/**/*.scss.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
