{
  "name": "figma-language-tester",
  "version": "2.0.0",
  "description": "A Figma plugin to test your UI in different languages",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-language-tester"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && build-figma-plugin",
    "clean": "rimraf '*.log' build manifest.json 'src/**/*.scss.d.ts'",
    "generate-menu-config": "ts-node scripts/generate-translate-menu-config.ts",
    "generate-translate-main-ts": "ts-node scripts/generate-translate-main-ts.ts > src/translate/main.ts",
    "fix": "eslint --fix '{scripts,src}/**/*.{ts,tsx}' && prettier --loglevel error --write 'src/**/*.scss' package.json tsconfig.json",
    "lint": "eslint '{scripts,src}/**/*.{ts,tsx}' && stylelint 'src/**/*.scss'",
    "watch": "yarn run clean && build-figma-plugin --watch"
  },
